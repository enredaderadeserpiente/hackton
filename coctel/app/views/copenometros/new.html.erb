<h1>New Copenometro</h1>

<%= render 'form', copenometro: @copenometro %>

<%= link_to 'Back', copenometros_path %>

<%= form_tag(copenometros_calcular_path) do %>

<table>
	<thead class="table">
		<tr>
			<th>Liquido</th>
			<th>Ml</th>
			<th>Grado</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @coponemotro_lista.each do |copenometro| %>
			<tr>
				<td><%= copenometro.liquido %></td>
				<td><%= copenometro.ml %></td>
				<input type="hidden" name="liquido[<%= copenometro.liquido %>][cantidad]" value="<%= copenometro.ml %>">
				<td><%= copenometro.grado %></td>
				<input type="hidden" name="liquido[<%= copenometro.liquido %>][grado]" value="<%= copenometro.grado %>">
				<td><%= link_to 'Edit', edit_copenometro_path(copenometro) %></td>
				<td><%= link_to 'Destroy', copenometro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
			</tr></br>
		<% end %>
	</tbody>
</table>


	<%= submit_tag "Calcular" %>

	
<% end %>
